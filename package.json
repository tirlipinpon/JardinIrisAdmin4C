    "lint-fix": "ng lint --fix",
    "build-prod": "node increment-version.js && ng build --configuration production",
    "build-dev": "node increment-version.js && ng build --configuration development",
    "serve-prod": "ng serve --configuration production",
    "serve-dev": "ng serve --configuration development"
  },
  "prettier": {
